programa
{
	
	funcao inicio()
	{
	const inteiro maxelefantes = 100
	inteiro c= 1
	
        escreva( c , " elefante incomoda muita gente! \n")
			escreva( c++ ," elefantes incomodam muito mais! \n")
			c++
			
		enquanto (c <= maxelefantes) {
			escreva( c , " elefantes incomodam muita gente! \n")
			escreva( c++ , " elefantes incomodam muito mais! \n")
			c++
			
		}
	}
}
