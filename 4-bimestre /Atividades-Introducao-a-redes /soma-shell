if [ $# -eq 2 ]
then
	let soma=$1+$2
	if [ $1 -gt $2 ]
	then
		echo $1 " é maior que " $2
		echo "a soma de " $1 "+" $2 " é igual a " $soma
	else
		if [ $2 -eq $1 ]
		then
			echo $2 " igual a " $1
			echo "a soma de " $1 "+" $2 " é igual a " $soma
		else
			echo $2 " é maior que " $1
			echo "a soma de " $1 "+" $2 " é igual a " $soma
		fi
	fi
else
    echo "Informe dois valores!!"
fi
